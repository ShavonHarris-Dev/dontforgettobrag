{
    "manifest_version": 3,
    "name": "Tracking Wins",
    "version": "1.0.0",
    "description": "A way to boost your confident and track your wins",
    "icons": {
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "permissions": [
        "storage"
    ],
    "actions": {
        "default_popup": "pop.html",
        "default_width": 400,
        "default_height": 600
    }
  }